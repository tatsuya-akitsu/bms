{
  "name": "bms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "prisma generate && prisma db seed && next build",
    "seed": "prisma db seed",
    "migrate": "prisma migrate dev --name init",
    "migrate:update": "prisma migrate dev",
    "deploy": "prisma migrate deploy",
    "applied": "prisma migrate resolve --applied '20231204133345_init'",
    "db:reset": "prisma migrate reset",
    "studio": "prisma studio",
    "start": "next start",
    "lint": "next lint",
    "codegen": "microcms-typescript src/schema src/types/response.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@types/firebase": "^3.2.1",
    "@vercel/postgres": "^0.5.1",
    "firebase": "^10.5.2",
    "next": "13.5.6",
    "openai": "^4.14.1",
    "prisma": "^5.5.2",
    "react": "^18",
    "react-dom": "^18",
    "recoil": "^0.7.7",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "server-only": "^0.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
